<%= provide(:title, 'Admin') %>

<div class="main-table">
  <%= form_for(:session, url: sessions_path) do |f| %>
    <h1>Admin</h1>

    <p class="error error_auth"><%= @error_auth %></p>
  
    <%= f.label :email %>
    <%= f.text_field :email %>
  
    <%= f.label :password %>
    <%= f.password_field :password %>
  
    <%= f.submit "Sing In", class: "btn" %>
  <% end %>
</div>